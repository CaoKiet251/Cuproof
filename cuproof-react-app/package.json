{
  "name": "cuproof-react-app",
  "version": "1.0.0",
  "description": "Cuproof Web Application with React.js and Node.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm run dev",
    "server": "cd server && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "cuproof",
    "react",
    "nodejs",
    "blockchain",
    "zero-knowledge"
  ],
  "author": "Cuproof Team",
  "license": "MIT"
}
